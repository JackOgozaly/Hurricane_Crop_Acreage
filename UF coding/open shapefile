import geopandas as gpd
df=gpd.read_file("/Users/xiaomeihai/Desktop/county/cb_2018_us_county_500k.shp")
FL=FL = df[(df["STATEFP"]=="12")]

import matplotlib.pyplot as plt
fig,ax=plt.subplots(figsize=(10,10))
FL.plot(ax=ax,column="GEOID", cmap="Blues")
ax.axis("off")
ax.set_title("FL counties", fontdict={"fontsize":"14","fontweight":"5"})

